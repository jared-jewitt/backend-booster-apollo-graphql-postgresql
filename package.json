{
  "name": "server-booster-graphql",
  "version": "1.0.0",
  "description": "Boilerplate for a GraphQL server",
  "author": "Jared Jewitt <jaredjewitt@gmail.com> (https://jared-jewitt.github.io/)",
  "license": "MIT",
  "keywords": [],
  "scripts": {
    "build": "babel src --out-dir=dist",
    "start": "cross-env NODE_ENV=development nodemon --exec babel-node src/index.js",
    "serve": "cross-env NODE_ENV=production node dist/index.js",
    "test": "jest",
    "test:update": "jest --updateSnapshot",
    "test:coverage": "jest --coverage",
    "lint": "eslint",
    "lint:fix": "eslint --fix"
  },
  "dependencies": {
    "apollo-server": "^2.17.0",
    "cross-env": "^7.0.2",
    "custom-env": "^2.0.1",
    "graphql": "^15.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.4",
    "@babel/node": "^7.10.5",
    "@babel/preset-env": "^7.11.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.7.0",
    "eslint-plugin-jest": "^23.20.0",
    "jest": "^26.4.2",
    "nodemon": "^2.0.4"
  }
}
