{
  "name": "backend-booster-graphql-postgresql",
  "version": "1.0.0",
  "description": "GraphQL + PostgreSQL boilerplate",
  "license": "MIT",
  "scripts": {
    "build": "webpack",
    "prod": "cross-env NODE_ENV=production PORT=${PORT:-7002} node build/server.js",
    "dev": "cross-env NODE_ENV=development PORT=${PORT:-7000} ts-node-dev -r tsconfig-paths/register src/server.ts",
    "test": "cross-env NODE_ENV=test PORT=${PORT:-7001} TYPE=all mocha",
    "test:e2e": "cross-env NODE_ENV=test PORT=${PORT:-7001} TYPE=e2e mocha",
    "test:unit": "cross-env NODE_ENV=test TYPE=unit mocha",
    "db:seed": "ts-node -r tsconfig-paths/register scripts/seed.ts",
    "db:wipe": "ts-node -r tsconfig-paths/register scripts/wipe.ts",
    "db:migrate:generate": "ts-node -r tsconfig-paths/register scripts/migrate.ts generate",
    "db:migrate:create": "ts-node -r tsconfig-paths/register scripts/migrate.ts create",
    "db:migrate:up": "ts-node -r tsconfig-paths/register scripts/migrate.ts up",
    "db:migrate:down": "ts-node -r tsconfig-paths/register scripts/migrate.ts down",
    "db:migrate:show": "ts-node -r tsconfig-paths/register scripts/migrate.ts show"
  },
  "dependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/chai": "^4.2.22",
    "@types/chai-as-promised": "^7.1.4",
    "@types/dotenv": "^8.2.0",
    "@types/faker": "^5.5.8",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/mocha": "^9.0.0",
    "@types/sinon": "^10.0.4",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "apollo-boost": "^0.4.9",
    "apollo-server": "^3.4.0",
    "bcrypt": "^5.0.1",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "class-validator": "^0.13.1",
    "cross-env": "^7.0.3",
    "cross-fetch": "^3.1.4",
    "dotenv": "^10.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-mocha": "^9.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "faker": "^5.5.3",
    "graphql": "^15.6.1",
    "jsonwebtoken": "^8.5.1",
    "mocha": "^9.1.2",
    "pg": "^8.7.1",
    "prettier": "^2.4.1",
    "reflect-metadata": "^0.1.13",
    "sinon": "^11.1.2",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.3.0",
    "type-graphql": "^1.1.1",
    "typeorm": "^0.2.38",
    "typescript": "^4.4.4",
    "webpack": "^5.58.2",
    "webpack-cli": "^4.9.0",
    "webpack-node-externals": "^3.0.0"
  },
  "devDependencies": {
    "husky": "^7.0.2",
    "lint-staged": "^11.2.3",
    "ts-node-dev": "^1.1.8"
  }
}
