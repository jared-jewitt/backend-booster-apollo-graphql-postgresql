{
  "name": "backend-booster-graphql-postgresql",
  "version": "1.0.0",
  "description": "Boilerplate for a GraphQL + PostgreSQL backend",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "serve": "cross-env NODE_ENV=production pm2 start build/index.js",
    "start": "cross-env NODE_ENV=development ts-node-dev --poll app/index.ts",
    "test": "cross-env NODE_ENV=test jest",
    "lint": "prettier --write \"**/*.{js,ts}\" && eslint --fix \"**/*.{js,ts}\"",
    "db:seed": "ts-node scripts/seed.ts",
    "db:wipe": "ts-node scripts/wipe.ts",
    "db:migrate": "ts-node scripts/migrate.ts"
  },
  "dependencies": {
    "apollo-server": "^2.19.0",
    "bcrypt": "^5.0.0",
    "class-validator": "^0.12.2",
    "graphql": "^15.4.0",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.4.2",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^1.1.0",
    "typeorm": "^0.2.28"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/graphql": "^14.5.0",
    "@types/jest": "^26.0.15",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^14.14.6",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "jest": "^26.6.1",
    "pm2": "^4.5.0",
    "prettier": "^2.1.2",
    "ts-jest": "^26.4.3",
    "ts-node": "^9.0.0",
    "ts-node-dev": "^1.0.0",
    "typescript": "^4.0.5"
  }
}
